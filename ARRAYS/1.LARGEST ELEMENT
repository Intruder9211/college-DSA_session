package Arrays;
public class largestelement {
    public static void main(String[] args) {
        int[] arr=new int[]{6,4,9,8,3,2};
        int max=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max)
            max=arr[i];
        }
        System.out.println("largest element"+max);
    }
    
}
