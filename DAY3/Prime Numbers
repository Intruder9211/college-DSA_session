package DsaSession.DAY3;

public class primeNum {
    public static void main(String[] args) {
        int n = 10;
        int num = 1, count =0;
        while(num <= n){
            for (int i = 1; i <= num; i++){
                if (num % i == 0){
                    count++;
                }
            }
            if (count == 2){
                System.out.println(""+num);
            }
            count = 0;
            num++;
        }
    }
    
}
